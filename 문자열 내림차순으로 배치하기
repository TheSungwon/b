import java.util.Arrays;

import java.util.Collections;



class Solution {

    public String solution(String s) {

        String answer = "";

        String[] array = s.split(""); //Zbcdefg

        Arrays.sort(array); //Zbcdefg

        Collections.reverse(Arrays.asList(array)); //gfedcbZ

        answer = String.join("",array); //"gfedcbZ"

        return answer;

    }

}

